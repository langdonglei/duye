page {
  font-family: Arial, Helvetica, sans-serif;
  background-color: rgb(145, 132, 132);
  
  
  
  
  
}

.h300 {
  height: 300rpx;
  overflow: hidden;
}

.h200{
  height: 200rpx;
  overflow: hidden;
}

.full {
  width: 100%;
  height: 100%;
}

.flex-wrap {
  display: flex;
  flex-wrap: wrap;
}

.box {
  width: 50%;
  padding: 1%;
  box-sizing: border-box;
}

.f12c {
  font-size: 12px;
  text-align: center;
}

.box-line {
  width: 100%;
  height: 300rpx;
  padding: 1%;
  box-sizing: border-box;
  overflow: hidden;
}

.title-line {
  text-align: center;
  font-size: 14px;
  line-height: 28px;
}